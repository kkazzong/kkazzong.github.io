package kr.co.kkaboard.server.service.impl;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import kr.co.kkaboard.server.dao.BoardDao;
import kr.co.kkaboard.server.dto.output.BoardOutputDto;
import kr.co.kkaboard.server.service.BoardService;

@Service("boardService")
public class BoardServiceImpl implements BoardService {

	@Resource(name="boardDao")
	private BoardDao boardDao;
	
	@Override
	public BoardOutputDto getBoard(int boardNo) {
		// TODO Auto-generated method stub
		return boardDao.getBoard(boardNo);
	}

}

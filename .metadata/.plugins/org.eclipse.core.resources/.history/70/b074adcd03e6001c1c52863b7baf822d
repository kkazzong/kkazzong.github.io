<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="imageDB">
	
	<insert id="addImage" parameterType="ReplyInputDto">
		insert into kka_image
			(
				title,
				file_name,
				file_path
			)
		values
			(
				#{title},
				#{file_name},
				#{file_path}
			)
	</insert>
</mapper>
